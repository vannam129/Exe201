{
  "name": "foodorderapp",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web-proxy": "expo start --web --clear --no-dev",
    "start-low": "node --max-old-space-size=1024 ./node_modules/.bin/expo start",
    "android-low": "node --max-old-space-size=1024 ./node_modules/.bin/expo start --android",
    "clean": "rmdir /s /q node_modules && del package-lock.json && npm install && npm cache clean --force",
    "clean-linux": "rm -rf node_modules && rm package-lock.json && npm install && npm cache clean --force",
    "reset": "expo start --clear",
    "reset-android": "expo start --clear --android"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^6.5.12",
    "@react-navigation/native": "^6.1.10",
    "@react-navigation/stack": "^6.3.21",
    "axios": "^1.6.7",
    "expo": "~52.0.41",
    "expo-status-bar": "~2.0.1",
    "jwt-decode": "^4.0.0",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  },
  "private": true
}
